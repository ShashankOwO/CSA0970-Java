import java.util.Scanner;
public class Main{
public  static void main(String[] args) {
    Scanner a=new Scanner(System.in);
    int[] denominations=new int[4];
    int[] numnotes=new int[4];
    int totalamount=0;
    for(int i=0;i<4;i++){
        System.out.print("Enter the"+(i+1)+"st dominations");
        denominations[i]=a.nextInt();
        System.out.print("Enetr the"+(i+1)+"st denomination no.of notes");
        numnotes[i]=a.nextInt();
    }
    for(int i=0;i<4;i++){
        totalamount+=denominations[i]*numnotes[i];
    }
    System.out.print("the total amount avaliable is:"+totalamount);

    }
}
