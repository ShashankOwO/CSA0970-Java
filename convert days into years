import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner a=new Scanner(System.in);
        int num=a.nextInt();
        int years=num/365;
        System.out.println("years: "+years);
        int weeks=(num%365)/7;
        System.out.println("weeks: "+weeks);
        int days=(num%365)%7;
        System.out.println("Days: "+days);
    }
}
